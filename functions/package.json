    "openai": "^4.77.3",
    "apify-client": "^2.9.5",
    "dns": "^0.2.2",
    "net": "^1.0.2"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "@types/node": "^22.10.5",
    "firebase-functions-test": "^3.3.0"
  },
  "private": true
}
